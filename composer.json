{
    "name": "dedecube/laravel-nova-profile",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova",
        "profile"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Dedecube\\Profile\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dedecube\\Profile\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
